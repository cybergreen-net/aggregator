{
  "country_lookup_method": "maxmind",
  "asn_lookup_method": "pyasn",
  "prefix_table": "./table-v4.txt",
  "pyasn_db": "./pyasn-ipv4.dat",
  "pyasn_db_path": "$CYBERGREEN_PYASN_DB_PATH",
  "maxmind_db": "GeoLite2-Country.mmdb",
  "maxmind_db_path": "$CYBERGREEN_MAXMIND_DB_PATH",
  "verbose": true,
  "out_sep": ",",
  "out_fields": ["ts", "ip", "risk_id", "asn", "cc"],
  "datapackage_path": "$CYBERGREEN_DATAPACKAGE_ROOT/datapackage.json",
  "source_file_regex": "parsed\\.(?P<year>\\d{4})(?P<month>\\d{2})(?P<day>\\d{2})\\.out\\.gz",
  "country_count_threshold": "$CYBERGREEN_AGG_COUNTRY_COUNT_THRESHOLD",
  "role_arn_etl2": "$CYBERGREEN_AWS_IAM_ROLE_ETL2",
  "role_arn_redshift": "$CYBERGREEN_AWS_IAM_ROLE_REDSHIFT",
  "test_aggregation_path": "$CYBERGREEN_AGGREGATION_PATH",
  "test_asn_aggregation_prefix": "asn-{}-{}.csv",
  "test_country_aggregation_prefix": "country-{}-{}.csv",
  "rds_uri": "postgres://$CYBERGREEN_STATS_RDS_USERNAME:$CYBERGREEN_STATS_RDS_PASSWORD@$CYBERGREEN_STATS_RDS_HOSTNAME.crovisjepxcd.eu-west-1.rds.amazonaws.com:5432/$CYBERGREEN_STATS_RDS_DBNAME",
  "redshift_uri": "postgres://$CYBERGREEN_REDSHIFT_USER:$CYBERGREEN_REDSHIFT_PASSWORD@$CYBERGREEN_REDSHIFT_CLUSTER_NAME.cqxchced59ta.eu-west-1.redshift.amazonaws.com:5439/$CYBERGREEN_BUILD_ENV",
  "redshift_cluster_name": "$CYBERGREEN_REDSHIFT_CLUSTER_NAME",
  "redshift_db_name": "$CYBERGREEN_REDSHIFT_NAME",
  "redshift_master_pwd": "$CYBERGREEN_REDSHIFT_PASSWORD",
  "redshift_node_type": "$CYBERGREEN_REDSHIFT_NODE_TYPE",
  "source_path": "$CYBERGREEN_SOURCE_ROOT",
  "scan_source_path": "$CYBERGREEN_SCAN_ROOT",
  "dest_path": "$CYBERGREEN_DEST_ROOT",
  "agg_path": "$CYBERGREEN_AGG_ROOT",
  "inventory": [{
      "name": "risk",
      "url": "https://raw.githubusercontent.com/cybergreen-net/refdata-risk/$CYBERGREEN_REF_BRANCH/datapackage.json"
    },
    {
      "name": "country",
      "url": "https://raw.githubusercontent.com/cybergreen-net/refdata-country/$CYBERGREEN_REF_BRANCH/datapackage.json"
    },
    {
      "name": "asn",
      "url": "https://raw.githubusercontent.com/cybergreen-net/refdata-asn/$CYBERGREEN_REF_BRANCH/datapackage.json"
    }
  ]
}

